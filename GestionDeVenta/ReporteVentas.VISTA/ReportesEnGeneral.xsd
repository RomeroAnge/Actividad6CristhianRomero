                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT    TOP (5) PRODUCTOS.NOMBREPRODUCTO, SUM(DETALLEVENTA.CANTIDAD) AS CANTIDADVENDIDA, COUNT(VENTAS.IDVENTA) AS VENTAS
FROM         PRODUCTOS INNER JOIN
                      DETALLEVENTA ON PRODUCTOS.IDPRODUCTO = DETALLEVENTA.IDPRODUCTO INNER JOIN
                      VENTAS ON DETALLEVENTA.IDVENTA = VENTAS.IDVENTA
